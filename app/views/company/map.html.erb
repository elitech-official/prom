<%= render 'company/show' do %>
<div id="map"></div>
    <script>

function initMap() {
  var myLatLng = {lat: 50.0060646, lng: 36.229062};

  var map = new google.maps.Map(document.getElementById('map'), {
    zoom: 4,
    center: myLatLng
  });

  var marker = new google.maps.Marker({
    position: myLatLng,
    map: map,
    title: 'Hello World!'
  });
}

    </script>
<% end %>
